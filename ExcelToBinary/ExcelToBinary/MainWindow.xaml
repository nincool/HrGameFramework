<Window x:Class="ExcelToBinary.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelToBinary"
        mc:Ignorable="d"
        Title="ExcelToBinary" Height="768" Width="1280">
    <Grid>
        <Label x:Name="labelSrcExcel" Content="原始Excel文件" HorizontalAlignment="Left" Margin="188,65,0,0" VerticalAlignment="Top" Width="687">

        </Label>
        <Button x:Name="button" Content="选择Excel" HorizontalAlignment="Left" Margin="80,68,0,0" VerticalAlignment="Top" Width="75" Click="OnClickOpenExcel"/>
        <Button x:Name="button1" Content="选择存储路径" HorizontalAlignment="Left" Margin="80,142,0,0" VerticalAlignment="Top" Width="75" Click="OnClickSelectSavePath"/>
        <TextBox x:Name="textSavePath" HorizontalAlignment="Left" Height="23" Margin="188,139,0,0" TextWrapping="Wrap" Text="存储路径" VerticalAlignment="Top" Width="732"/>
        <Button x:Name="button2" Content="转换" HorizontalAlignment="Left" Margin="187,187,0,0" VerticalAlignment="Top" Width="75" Click="OnClickTranslate"/>
        <Button x:Name="button3" Content="读取" HorizontalAlignment="Left" Margin="83,229,0,0" VerticalAlignment="Top" Width="75" Click="OnClickReadByteData"/>
    </Grid>
</Window>
